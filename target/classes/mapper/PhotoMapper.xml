<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.myweb.fxj.mapper.module.memory.PhotographMapper">
    <select id="getPhotographByQuery" resultType="Photograph">
        select * from memory_photograph where id = #{id} and is_deleted = 0 and name = #{name}
    </select>
</mapper>